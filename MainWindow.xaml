        <Label x:Name="GreenSliderValue" Content="{Binding Value, ElementName=GreenSlider, StringFormat=\{0:F0\}}" HorizontalAlignment="Left" Margin="640,134,0,0" VerticalAlignment="Top" Width="54" Background="#FF1A1A1A" Foreground="White" BorderBrush="#FF212138" Height="26" Grid.Column="1" Grid.ColumnSpan="2" />
