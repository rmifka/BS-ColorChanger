        <Button Style="{DynamicResource RoundedButtons}" x:Name="EnvironmentInner" Content="Environment Inner" Click="Click_ColorLeftOrRight" Background="#212138" Margin="395,10,0,0" Height="30" Width="100" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" FontFamily="Adobe Gothic Std L" FontSize="10" Grid.Column="1" />
        <Button Style="{DynamicResource RoundedButtons}" x:Name="Wall" Content="Obstacle Color" Click="Click_ColorLeftOrRight" Background="#212138" Height="30" Width="100" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,10,0,0" Foreground="White" FontFamily="Adobe Gothic Std L" FontSize="10" FontWeight="Normal" Grid.Column="1" />
        <Button Style="{DynamicResource RoundedButtons}" x:Name="Color1" Content="Color 1" Click="Color_Click" Background="#FF56FFFF" FontFamily="Adobe Gothic Std L" FontSize="10" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#FF272222" Grid.ColumnSpan="2" Margin="18,295,0,0" />
        <Button Style="{DynamicResource RoundedButtons}" x:Name="Color2" Content="Color 2" Click="Color_Click" FontSize="10" FontFamily="Adobe Gothic Std L" Height="30" Width="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF212138" Foreground="White" Margin="16,295,0,0" Grid.Column="1" />
        <Button Style="{DynamicResource RoundedButtons}" x:Name="Color3" Content="Color 3" Click="Color_Click" FontSize="10" FontFamily="Adobe Gothic Std L" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF212138" Foreground="White" Grid.Column="1" Margin="76,295,0,0" />
        <Button Style="{DynamicResource RoundedButtons}" x:Name="Color4" Content="Color 4" Click="Color_Click" FontFamily="Adobe Gothic Std L" FontSize="10" Height="30" Width="50" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#FF212138" Foreground="White" Grid.Column="1" Margin="136,295,0,0" />
        <TextBlock HorizontalAlignment="Left" Height="22" Margin="61,193,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41" Foreground="White" FontFamily="Adobe Gothic Std L" Grid.Column="1" Text="Alpha" />
        <Slider x:Name="AlphaSlider" Style="{DynamicResource Horizontal_Slider}" Margin="107,197,44,184" Maximum="255" SmallChange="1" LargeChange="10" IsSnapToTickEnabled="True" IsSelectionRangeEnabled="True" ValueChanged="AlphaSlider_ValueChanged" Background="#003A3A3A" Grid.Column="1" />
        <Label x:Name="AlphaSliderValue" Content="{Binding Value, ElementName=AlphaSlider, StringFormat=\{0:F0\}}" HorizontalAlignment="Left" Margin="640,191,0,0" VerticalAlignment="Top" Width="54" Background="#FF1A1A1A" Foreground="White" BorderBrush="#FF212138" Height="26" Grid.Column="1" Grid.ColumnSpan="2" />
        <Button Style="{DynamicResource RoundedButtons}" Content="Save to File" HorizontalAlignment="Left" Margin="632,10,0,0" VerticalAlignment="Top" Click="SaveToUserClick" Grid.Column="1" Width="90" Height="30" FontFamily="Adobe Gothic Std L" FontSize="10" FontWeight="Bold" Foreground="White" OpacityMask="Red" Cursor="Help" Grid.ColumnSpan="2">
            <Button.Background>
                <LinearGradientBrush EndPoint="0,0" StartPoint="1,1" SpreadMethod="Pad">
                    <LinearGradientBrush.RelativeTransform>
                        <TransformGroup>
                            <ScaleTransform CenterY="0.5" CenterX="0.5" />
                            <SkewTransform AngleX="0" CenterX="0.5" CenterY="0.5" AngleY="3" />
                            <RotateTransform CenterX="0.5" CenterY="0.5" />
                            <TranslateTransform />
                        </TransformGroup>
                    </LinearGradientBrush.RelativeTransform>
                    <GradientStop Color="Red" Offset="1" />
                    <GradientStop Color="#FF00CAFF" Offset="0" />
                </LinearGradientBrush>
            </Button.Background>
        </Button>
