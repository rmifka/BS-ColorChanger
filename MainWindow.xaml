        <Button Style="{DynamicResource RoundedButtons}" x:Name="LeftSaber" Content="Left Saber" Click="Click_ColorLeftOrRight" Width="100" Height="30" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="80,10,0,0" Background="#FF56FFFF" Foreground="#FF272222" FontFamily="Adobe Gothic Std L" FontSize="10" Grid.Column="1" />
