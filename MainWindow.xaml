        <TextBlock HorizontalAlignment="Left" Height="22" Margin="62,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="41" Foreground="White" FontFamily="Adobe Gothic Std L" Grid.Column="1" Text="Green" />
        <TextBlock HorizontalAlignment="Left" Height="22" Margin="62,164,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="39" Foreground="White" FontFamily="Adobe Gothic Std L" Grid.Column="1"><Run Text="Blue" /><Run Text=" " /></TextBlock>
        <Slider x:Name="RedSlider" Style="{DynamicResource Horizontal_Slider}" Margin="107,107,44,277" Maximum="255" LargeChange="10" SmallChange="1" IsSnapToTickEnabled="True" ValueChanged="RedSlider_ValueChanged" Background="{x:Null}" Grid.Column="1" />
        <Slider x:Name="GreenSlider" Style="{DynamicResource Horizontal_Slider}" Margin="107,138,44,248" Maximum="255" SmallChange="1" LargeChange="10" IsSnapToTickEnabled="True" ValueChanged="GreenSlider_ValueChanged" Grid.Column="1" />
