            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform />
                    <SkewTransform />
                    <RotateTransform />
                    <TranslateTransform X="1" Y="0.1" />
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
